{
  "name": "react-babel-gulp",
  "version": "1.0.0",
  "description": "gulp-babel react browserify babrlify es6 bower jquery bootstrap local-web-server gulp-livereload",
  "author": "Maksim Kostromin",
  "main": "src/main.jsx",
  "local-web-server": {
    "port": 3000,
    "forbid": [
      "*.json",
      "*.ts",
      "*.md",
      "*.yml",
      "*.babel.js",
      ".gitignore"
    ],
    "no-cache": true
  },
  "babel": {
    "presets": ["es2015"]
  },
  "config": {
    "bower_directory": "src/vendors"
  },
  "scripts": {
    "gulp": "gulp",
    "bower": "bower",
    "pregulp": "npm i",
    "rm": "rm -fr dist",
    "test": "npm run gulp",
    "prestart": "npm test",
    "start": "gulp watch & ws -d dist",
    "postinstall": "bower install -p --config.interactive=false"
  },
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.5.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "bootstrap": "^3.3.6",
    "bower": "^1.7.7",
    "browserify": "^13.0.0",
    "colors": "^1.1.2",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-concat": "^2.6.0",
    "gulp-livereload": "^3.8.1",
    "local-web-server": "^1.1.1",
    "gulp-sourcemaps": "^1.6.0",
    "jquery": "^2.2.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "require-dir": "^0.3.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "dependencies": {},
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daggerok/react-babel-gulp.git"
  },
  "keywords": [
    "gulp-babel",
    "es6",
    "bower",
    "react",
    "browserify",
    "babrlify",
    "jquery",
    "bootstrap",
    "local-web-server",
    "gulp-livereload"
  ],
  "bugs": {
    "url": "https://github.com/daggerok/react-babel-gulp/issues"
  },
  "homepage": "https://github.com/daggerok/react-babel-gulp#readme"
}
