{
  "name": "react-babel-gulp",
  "version": "1.0.0",
  "description": "react gulp bower browserify babel babrlify es6 jquery bootstrap",
  "author": "Maksim Kostromin",
  "main": "src/main.js",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-runtime",
      "react-html-attrs",
      "transform-class-properties",
      "transform-decorators-legacy"
    ]
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ]
    ]
  },
  "config": {
    "bower_directory": "src/vendors"
  },
  "scripts": {
    "bower": "bower",
    "postinstall": "bower install -p",
    "rm": "rm -fr dist",
    "gulp": "gulp",
    "start": "gulp watch",
    "prestart": "npm install"
  },
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.5.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "bootstrap": "^3.3.6",
    "bower": "^1.7.7",
    "browserify": "^13.0.0",
    "colors": "^1.1.2",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-concat": "^2.6.0",
    "gulp-connect": "^2.3.1",
    "gulp-sourcemaps": "^1.6.0",
    "jquery": "^2.2.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "require-dir": "^0.3.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "dependencies": {},
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daggerok/react-babel-gulp.git"
  },
  "keywords": [
    "react",
    "gulp",
    "bower",
    "browserify",
    "babel",
    "babrlify",
    "es6",
    "jquery",
    "bootstrap"
  ],
  "bugs": {
    "url": "https://github.com/daggerok/react-babel-gulp/issues"
  },
  "homepage": "https://github.com/daggerok/react-babel-gulp#readme"
}
